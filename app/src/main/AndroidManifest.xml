<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="play.gator.farmgator">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/farmlogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/farmlogo_foreground"
        android:supportsRtl="true"
        android:theme="@style/Theme.FarmGator">
        <activity
            android:name=".Onboard.FarmDetails"
            android:exported="true"
            android:theme="@style/NoAction" >

        </activity>
        <activity
            android:name=".Onboard.OnboardFirst"
            android:exported="false"
            android:theme="@style/NoAction" />
        <activity
            android:name=".Dashboard.Dashboard"
            android:exported="false"
            android:theme="@style/NoAction" />
        <activity
            android:name=".SplashScreen.Splash"
            android:exported="true"
            android:theme="@style/NoAction">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name=".SalesPerson.Login"
            android:exported="false"
            android:theme="@style/NoAction" />
        <activity
            android:name=".SalesPerson.Registration"
            android:exported="false"
            android:theme="@style/NoAction" />
        <activity
            android:name=".MainActivity"
            android:exported="true" />
    </application>

</manifest>